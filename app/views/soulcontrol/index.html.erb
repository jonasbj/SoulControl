<body ng-controller="AppController" class="page-md page-header-fixed page-sidebar-closed-hide-logo page-container-bg-solid page-sidebar-closed-hide-logo page-on-load" ng-class="{'page-sidebar-closed': settings.layout.pageSidebarClosed}">

<!-- BEGIN PAGE SPINNER -->
<div ng-spinner-bar class="page-spinner-bar">
  <div class="bounce1"></div>
  <div class="bounce2"></div>
  <div class="bounce3"></div>
</div>
<!-- END PAGE SPINNER -->

    <!-- BEGIN HEADER -->
<div data-ng-include="'tpl/header.html'" data-ng-controller="HeaderController" class="page-header md-shadow-z-1-i navbar navbar-fixed-top">
</div>
<!-- END HEADER -->

<div class="clearfix">
</div>

<!-- BEGIN CONTAINER -->
<div class="page-container">
  <!-- BEGIN SIDEBAR -->
  <div data-ng-include="'tpl/sidebar.html'" data-ng-controller="SidebarController" class="page-sidebar-wrapper">
  </div>
  <!-- END SIDEBAR -->
  <div class="page-content-wrapper">
    <div class="page-content">
      <!-- BEGIN PAGE HEAD -->
      <div data-ng-include="'tpl/page-head.html'" data-ng-controller="PageHeadController" class="page-head">
      </div>
      <!-- END PAGE HEAD -->
      <!-- BEGIN ACTUAL CONTENT -->
      <div ui-view class="fade-in-up">
      </div>
      <!-- END ACTUAL CONTENT -->
    </div>
  </div>
</div>
<!-- END CONTAINER -->

<!-- BEGIN FOOTER -->
<div data-ng-include="'tpl/footer.html'" data-ng-controller="FooterController" class="page-footer">
</div>
<!-- END FOOTER -->

<!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->

<!-- BEGIN CORE JQUERY PLUGINS -->
<!--[if lt IE 9]>
<%= javascript_include_tag 'global/plugins/respond.min' %>
<%= javascript_include_tag 'global/plugins/excanvas.min' %>

<![endif]-->

<%= javascript_include_tag 'global/plugins/jquery.min' %>
<%= javascript_include_tag 'global/plugins/jquery-migrate.min' %>
<%= javascript_include_tag 'global/plugins/bootstrap/js/bootstrap.min' %>
<%= javascript_include_tag 'global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min' %>
<%= javascript_include_tag 'global/plugins/jquery-slimscroll/jquery.slimscroll.min' %>
<%= javascript_include_tag 'global/plugins/jquery.blockui.min' %>
<%= javascript_include_tag 'global/plugins/jquery.cokie.min' %>
<%= javascript_include_tag 'global/plugins/uniform/jquery.uniform.min' %>
<!-- END CORE JQUERY PLUGINS -->

<!-- BEGIN CORE ANGULARJS PLUGINS -->
<%= javascript_include_tag 'global/plugins/angularjs/angular.min' %>
<%= javascript_include_tag 'global/plugins/angularjs/angular-sanitize.min' %>
<%= javascript_include_tag 'global/plugins/angularjs/angular-touch.min' %>
<%= javascript_include_tag 'global/plugins/angularjs/plugins/angular-ui-router.min' %>
<%= javascript_include_tag 'global/plugins/angularjs/plugins/ocLazyLoad.min' %>
<%= javascript_include_tag 'global/plugins/angularjs/plugins/ui-bootstrap-tpls.min' %>
<!-- END CORE ANGULARJS PLUGINS -->

<!-- BEGIN APP LEVEL ANGULARJS SCRIPTS -->
<%= javascript_include_tag 'app' %>
<%= javascript_include_tag 'directives' %>
<script src="assets/app.js" type="text/javascript"></script>
<script>
    <%= render "soulcontrol_service.js.erb" %>
</script>
<script src="assets/directives.js" type="text/javascript"></script>
<!-- END APP LEVEL ANGULARJS SCRIPTS -->

<!-- BEGIN APP LEVEL JQUERY SCRIPTS -->
<%= javascript_include_tag 'global/scripts/metronic' %>
<%= javascript_include_tag 'admin/layout/scripts/layout' %>
<%= javascript_include_tag 'admin/layout/scripts/demo' %>
<!-- END APP LEVEL JQUERY SCRIPTS -->

<script type="text/javascript">
    /* Init Metronic's core jquery plugins and layout scripts */
    $(document).ready(function() {
        Metronic.init(); // Run metronic theme
        Metronic.setAssetsPath('assets/'); // Set the assets folder path
    });
</script>
<!-- END JAVASCRIPTS -->
</body>

